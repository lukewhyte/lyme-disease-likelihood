<lyme-data-overlay counties="{counties}" 
				   curr-county="{currCounty}" 
				   date-range="{dateRange}"
				   is-map-view="{appState.isMapView}"  
				   map="{map}"
				   is-loading="{isLoading}"></lyme-data-overlay>

{{#if isLoading}}
	<div id="loading"><img src="/images/loading.gif" /></div>
{{/if}}