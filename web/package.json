    "buildConfig": {
      "map": {
        "can/util/util": "can/util/domless/domless"
      }
    }
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-concat-css": "^0.3.1",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-express-server": "^0.5.1",
    "grunt-sass-import": "^0.1.0",
    "grunt-shell-spawn": "^0.3.9",
    "steal": "^0.11.1",
    "steal-tools": "^0.11.0"
  },
  "dependencies": {
    "c3": "^0.4.11-rc4",
    "jquery": "^2.1.4",
    "leaflet": "^0.7.5",
    "topojson": "^1.6.19"
